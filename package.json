{
  "name": "@mate-academy/emmet-monaco",
  "version": "1.0.2",
  "description": "emmet plugin for monaco-editor",
  "keywords": [
    "emmet",
    "emmet-plugin",
    "monaco-editor"
  ],
  "main": "dist/emmet-monaco.common.js",
  "module": "dist/emmet-monaco.esm.js",
  "typings": "index.d.ts",
  "scripts": {
    "watch": "rollup -c --watch",
    "build": "rollup -c",
    "prepublishOnly": "rm -rf ./dist && npm run build",
    "postpublish": "rm -rf dist",
    "patch": "npm version patch && npm publish"
  },
  "sideEffects": false,
  "license": "MIT",
  "author": "Troy",
  "homepage": "https://github.com/troy351/emmet-monaco-es",
  "peerDependencies": {
    "@mate-academy/monaco-editor": ">=0.2.7<0.4.0"
  },
  "dependencies": {
    "@emmetio/abbreviation": "^0.7.0",
    "@emmetio/css-abbreviation": "^0.4.0",
    "@emmetio/css-snippets-resolver": "^0.4.0",
    "@emmetio/html-snippets-resolver": "^0.1.4",
    "@emmetio/html-transform": "^0.3.10",
    "@emmetio/lorem": "^1.0.2",
    "@emmetio/markup-formatters": "^0.4.1",
    "@emmetio/output-profile": "^0.1.6",
    "@emmetio/snippets": "^0.2.12",
    "@emmetio/snippets-registry": "^0.3.1",
    "@emmetio/stylesheet-formatters": "^0.2.1",
    "@emmetio/variable-resolver": "^0.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^8.1.1",
    "rollup": "^2.38.5",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  }
}
